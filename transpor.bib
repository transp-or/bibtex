	issn = {1573-2975},
	journal = {Environment, Development and Sustainability},
	keywords = {Design thinking, Interventions, Real-world laboratories, Transdisciplinarity, Transdisciplinary research, Urban sustainability transformation},
	language = {en},
	month = sep,
	title = {Designing interventions for sustainable change in a real-world laboratory},
