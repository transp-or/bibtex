	doi = {10.3141/2429-03},
	file = {:Users/negarrezvani/Library/Application Support/Mendeley Desktop/Downloaded/Gupta et al. - 2014 - Incorporation of escorting children to school in modeling individual daily activity patterns of household members.pdf:pdf},
	isbn = {9780309295239},
	issn = {03611981},
	journal = {Transp. Res. Rec.},
	number = {1},
	pages = {20--29},
	title = {{Incorporation of escorting children to school in modeling individual daily activity patterns of household members}},
	volume = {2429},
	year = {2014},
	bdsk-url-1 = {https://doi.org/10.3141/2429-03}}

@article{Habib2017,
	abstract = {The paper presents the results of an investigation on daily activity-travel scheduling behaviour of older people by using an advanced econometric model and a household travel survey, collected in the National Capital Region (NCR) of Canada in 2011. The activity-travel scheduling model considers a dynamic time--space constrained scheduling process. The key contribution of the paper is to reveal daily activity-travel scheduling behaviour through a comprehensive econometric framework. The resulting empirical model reveals many behavioural details. These include the role that income plays in moderating out-of-home time expenditure choices of older people. Older people in the highest and lowest income categories tend to have lower variations in time expenditure choices than those in middle-income categories. Overall, the time expenditure choices become more stable with increasing age, indicating that longer activity durations and lower activity frequency become more prevalent with increasing age. Daily activity type and location choices reveal a clear random utility-maximizing rational behaviour of older people. It is clear that increasing spatial accessibility to various activity locations is a crucial factor in defining daily out-of-home activity participation of older people. It is also clear that the diversity of out-of-home activity type choices reduces with increasing age and older people are more sensitive to auto travel time than to transit or non-motorized travel time.},
	author = {Habib, Khandker M.Nurul and Hui, Vivian},
	date-added = {2023-12-13 14:13:40 +0100},
	date-modified = {2023-12-13 14:13:40 +0100},
	doi = {10.1007/s11116-015-9667-1},
	file = {:Users/negarrezvani/Library/Application Support/Mendeley Desktop/Downloaded/Habib, Hui - 2017 - An activity-based approach of investigating travel behaviour of older people.pdf:pdf},
	journal = {Transportation (Amst).},
	keywords = {Activity duration,Activity-travel scheduling,Elderly mobility,Out-of-home activity participation,Trip chaining},
	number = {3},
	pages = {555--573},
	publisher = {Springer US},
	title = {{An activity-based approach of investigating travel behaviour of older people}},
	volume = {44},
	year = {2017},
	bdsk-url-1 = {https://doi.org/10.1007/s11116-015-9667-1}}

@techreport{Hackney2005,
	abstract = {A component of work and non-work travel may be plausibly explained$\backslash$nby the need for individuals to be co-present in order socially interact.$\backslash$nSuccessful socializing requires maintaining and exploiting relationships,$\backslash$nfulfilling obligations, and perhaps optimization of contacts. A social$\backslash$nnetwork is a way to map relationships and to quantify conduits of$\backslash$ninformation, services, opportunity, and power. People must be able$\backslash$nto navigate both their relationships and physical space in order$\backslash$nto find other people and facilities which can provide the services$\backslash$nthey need. The result is that travel behavior and social structures$\backslash$naffect each other. This research examines hypotheses of the interactions$\backslash$nbetween social networks and trip generation. Models of social networks$\backslash$nshould be used by planners if their effects are significant and distinguishable$\backslash$nfrom factors currently incorporated into transportation models. A$\backslash$nmulti-agent-based network evolution model is presented in which social$\backslash$nvisits are generated by a set of attraction parameters which include$\backslash$nagent attributes, travel impedance, and descriptors of the actors'$\backslash$nposition in the social network topology. The trips are constrained$\backslash$nby the agent's travel budget, and dynamic equilibrium is maintained$\backslash$nby removing or weakening old links. The destination choice is thus$\backslash$na tradeoff between cost and the need for co-presence, marked by correlations$\backslash$nbetween actors in the changing social network. The interactions are$\backslash$nmonitored using a statistical toolbox from social network analysis$\backslash$nand placed in the context of findings from network science literature$\backslash$non network evolution, as well as from empirical work in sociology$\backslash$non real social networks. The behavior model used for social networking$\backslash$nis described, and results from an ensemble run are presented. A sample$\backslash$ncase of a transportation ``improvement'' is also presented, to illustrate$\backslash$nthe way in which the social network adapts to changes in transportation$\backslash$ncosts that ease spatial searches},
	author = {Hackney, J K},
	date-added = {2023-12-13 14:13:40 +0100},
	date-modified = {2023-12-13 14:13:40 +0100},
	file = {:Users/negarrezvani/Downloads/eth-28870-01.pdf:pdf},
	number = {335},
	title = {{Coevolving Social and Transportation Networks}},
	year = {2005}}

