	isbn = {2210-6707},
	journal = {Sustainable Cities and Society},
	keywords = {Active travel; Digital social market; Smart Cities; Social network; Sustainability},
	pages = {103595},
	title = {Using digital social market applications to incentivise active travel: Empirical analysis of a smart city initiative},
	url = {https://www.sciencedirect.com/science/article/pii/S221067072100860X},
