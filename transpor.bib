	keywords = {Evaluation, Mobility transition, Socio-ecological transformation, Transdisciplinary labs, Transdisciplinary research},
	month = apr,
	pages = {104153},
	title = {Mobility transition through labs in real-world contexts. {The} contribution of transdisciplinary approaches in creating sustainable transportation systems},
