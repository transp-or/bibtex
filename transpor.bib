	keywords = {Household travel survey; GPS-based surveys; Sensor-based data collection; Future mobility sensing; Activity-based models; Machine learning; smartphone-based survey},
	pages = {413--429},
	title = {Insights on data quality from a large-scale application of smartphone-based travel survey technology in the {P}hoenix metropolitan area, {A}rizona, {USA}},
