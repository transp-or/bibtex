	date-modified = {2025-08-29 10:44:04 +0200},
	doi = {https://doi.org/10.1016/j.jocm.2017.01.007},
	isbn = {1755-5345},
	journal = {Journal of Choice Modelling},
	keywords = {Discrete choice; GHK simulator; Sparse grid integration; Composite marginal likelihood (CML) method; MACML estimation; Bayesian Markov Chain Monte Carlo (MCMC)},
	pages = {9--20},
	title = {Simulation evaluation of emerging estimation techniques for multinomial probit models},
	url = {https://www.sciencedirect.com/science/article/pii/S1755534515300890},
	volume = {23},
	year = {2017},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S1755534515300890},
