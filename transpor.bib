	journal = {Transportation Research Part A: Policy and Practice},
	language = {en},
	month = feb,
	pages = {1--8},
	title = {Addressing transport policy challenges through {Policy}-{Packaging}},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0965856413002000},
	urldate = {2025-04-24},
