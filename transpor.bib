	issn = {23249943},
	journal = {Transp. A Transp. Sci.},
	keywords = {Lagrangian relaxation,activity scheduling,activity-based travel demand models,inventory routing problems,space--time--needs,time geography,vehicle routing problems},
	number = {3},
	pages = {243--269},
	title = {A multi-day activity-based inventory routing model with space--time--needs constraints},
	url = {https://doi.org/10.1080/23249935.2014.958120},
