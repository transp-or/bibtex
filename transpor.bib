	file = {Full Text PDF:/Users/matthieugautrot/Zotero/storage/4C6VB883/Bergmann et al. - 2021 - Transdisciplinary sustainability research in real-.pdf:application/pdf},
	issn = {1862-4057},
	journal = {Sustainability Science},
	keywords = {Educational Research, Methods, Mixed Methods, Practice-as-Research, Quality of Life Research, Real-world lab, Success factors, Sustainability, Sustainability transitions, Third Sector Research, Transdisciplinarity, Transformative research},
	language = {en},
	month = mar,
	number = {2},
