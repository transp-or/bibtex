	issn = {1468-2451},
	journal = {International Social Science Journal},
	keywords = {action research, behavioural changes, gamified application, soft intervention, sustainable mobility},
	language = {en},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/issj.12575},
	number = {n/a},
	title = {Application of {Action} {Research} {Methodology} in the {Development} of {Measures} to {Promote} {Sustainable} {Mobility}},
