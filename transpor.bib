	keywords = {Route choice modeling; Choice-set generation; Conditional variational autoencoder; Implicit availability/perception; Data- and model-driven choice model},
	pages = {104618},
	title = {Enhancing choice-set generation and route choice modeling with data- and knowledge-driven approach},
	url = {https://www.sciencedirect.com/science/article/pii/S0968090X24001396},
	volume = {162},
	year = {2024},
