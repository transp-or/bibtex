	keywords = {Discrete-continuous system, Multiple discreteness, Kuhn--Tucker demand systems, Mixed discrete choice, Random utility maximization},
	note = {A Tribute to the Career of Frank Koppelman},
	number = {3},
	pages = {274-303},
	title = {The multiple discrete-continuous extreme value ({MDCEV}) model: Role of utility function parameters, identification considerations, and model extensions},
	url = {https://www.sciencedirect.com/science/article/pii/S0191261507000677},
	volume = {42},
