	number = {1},
	pages = {193--214},
	title = {An improvement in {MATSim} computing time for large-scale travel behaviour microsimulation},
	url = {https://doi.org/10.1007/s11116-019-10048-0},
	volume = {48},
	year = {2021},
	bdsk-url-1 = {https://doi.org/10.1007/s11116-019-10048-0}}

