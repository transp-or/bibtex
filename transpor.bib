	doi = {10.1016/j.jocm.2018.05.004},
	isbn = {1755-5345},
	journal = {Journal of Choice Modelling},
	keywords = {Activity-travel demand; Modeling weekly activity schedules; Time allocation; Multiple discrete-continuous probit model; Weekday and weekend travel},
	pages = {56--70},
	title = {Understanding activity engagement across weekdays and weekend days: A multivariate multiple discrete-continuous modeling approach},
	volume = {28},
	year = {2018},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S1755534517301525},
	bdsk-url-2 = {https://doi.org/10.1016/j.jocm.2018.05.004}}

