@article{Ettema2000,
	abstract = {The estimation results are reported of a model of activity scheduling behavior, based on an interactive computer experiment. The model combines the positive features of discrete choice modeling and computational process modeling of activity scheduling decisions. In particular, it describes the pre-trip planning phase, in which individuals decide which activities to perform, where, when, in which sequence and how to travel to the various activity sites. The results indicate that when scheduling their activities, subjects seem to trade-off attributes of activities (time constraints, duration) against attributes of the schedule (time spent on activities, overall travel time).},
