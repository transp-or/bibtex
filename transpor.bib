	doi = {https://doi.org/10.1016/j.jocm.2017.11.003},
	isbn = {1755-5345},
	journal = {Journal of Choice Modelling},
	keywords = {Machine learning; Latent behaviour models; Decision making; Financial instruments; Consumer behaviour},
	pages = {152--168},
	title = {Discriminative conditional restricted Boltzmann machine for discrete choice and latent variable modelling},
