@inbook{Miller2021,
	abstract = {Activity-based analysis (ABA) is an approach to understanding transportation, communication, urban, and related social and physical systems using individual actions in space and time as the basis. Although the conceptual foundations, theory, and methodology have a long tradition, until recently an aggregate trip-based approach dominated transportation science and planning. Changes in the business and policy environment for transportation and the increasingly availability of disaggregate mobility data have led to ABA emerging as the dominant approach. This chapter reviews the ABA conceptual foundations and methodologies. ABA techniques include data-driven methods that analyze mobility data directly as well as develop inputs for ABA modeling. ABA models include econometric models, rule-based models and microsimulation/agent-based models. This chapter concludes by identifying major research frontiers in ABA.},
	address = {Berlin, Heidelberg},
	author = {Miller, Harvey J.},
	booktitle = {Handbook of Regional Science},
	doi = {10.1007/978-3-662-60723-7_106},
