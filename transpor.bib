	journal = {Annual Review of Environment and Resources},
	keywords = {values},
	number = {Volume 41, 2016},
	pages = {277-292},
	publisher = {Annual Reviews},
	title = {Values, Norms, and Intrinsic Motivation to Act Proenvironmentally},
