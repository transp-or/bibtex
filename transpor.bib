	issn = {09658564},
	journal = {Transp. Res. Part A Policy Pract.},
	keywords = {Activity-based model,Ageing population,Group decision,Online-to-offline services,Walkability},
	pages = {172--194},
	publisher = {Elsevier},
	title = {{Modelling intra-household interactions in time-use and activity patterns of retired and dual-earner couples}},
