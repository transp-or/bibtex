	isbn = {2666-691X},
	journal = {Transportation Engineering},
	keywords = {Large-scale traffic simulation; Microscopic traffic simulation; Activity-based models; Trip-based model; Travel demand generation},
	pages = {100329},
	title = {Large-scale activity-based demand generation modeling: A literature review and exploration of potential approaches},
	url = {https://www.sciencedirect.com/science/article/pii/S2666691X25000296},
	volume = {20},
