	journal = {Transportation Research Part B: Methodological},
	number = {2},
	pages = {147--163},
	title = {On the use of a Modified Latin Hypercube Sampling ({MLHS}) method in the estimation of a Mixed Logit Model for vehicle choice},
	url = {https://www.sciencedirect.com/science/article/pii/S0191261505000500},
	volume = {40},
