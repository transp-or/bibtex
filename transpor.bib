	isbn = {0965-8564},
	journal = {Transportation Research Part A: Policy and Practice},
	keywords = {Driver's license use; Young adults; Transit accessibility; Hazard model; Land use; Transportation},
	pages = {374--386},
	title = {Modelling the choice and timing of acquiring a driver's license: Revelations from a hazard model applied to the University students in Toronto},
	url = {https://www.sciencedirect.com/science/article/pii/S096585641630653X},
	volume = {118},
	year = {2018},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S096585641630653X},
